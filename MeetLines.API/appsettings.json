{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=5432;Database=${DB_DATABASE};Username=${DB_USERNAME};Password=${DB_PASSWORD}"
  },
  "Jwt": {
    "SecretKey": "${JWT_SECRET}",
    "Issuer": "MeetLines",
    "Audience": "MeetLines",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7
  },
  "Email": {
    "SmtpHost": "${EMAIL_SMTP_HOST}",
    "SmtpPort": "${EMAIL_SMTP_PORT}",
    "SmtpUser": "${EMAIL_SMTP_USER}",
    "SmtpPassword": "${EMAIL_SMTP_PASSWORD}",
    "FromEmail": "${EMAIL_FROM}",
    "FromName": "${EMAIL_FROM_NAME}"
  },
  "Frontend": {
    "Url": "${FRONTEND_URL}"
  },
  "GeoIp": {
    "DatabasePath": "GeoIp/GeoLite2-City.mmdb",
    "CacheTtlSeconds": 86400
  },
  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    },
    "Facebook": {
      "AppId": "${FB_APP_ID}",
      "AppSecret": "${FB_APP_SECRET}"
    }
  },
  "MercadoPago": {
    "AccessToken": "${MERCADOPAGO_ACCESS_TOKEN}"
  },
  "Cloudinary": {
    "CloudName": "${CLOUDINARY_CLOUD_NAME}",
    "ApiKey": "${CLOUDINARY_API_KEY}",
    "ApiSecret": "${CLOUDINARY_API_SECRET}"
  },
  "Multitenancy": {
    "BaseDomain": "${MULTITENANCY_BASE_DOMAIN}",
    "Protocol": "${MULTITENANCY_PROTOCOL}",
    "Enabled": true
  }
}