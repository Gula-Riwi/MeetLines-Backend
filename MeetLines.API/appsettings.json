{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_DATABASE};Username=${DB_USERNAME};Password=${DB_PASSWORD}"
  },

  "Jwt": {
    "SecretKey": "${JWT_SECRET}",
    "Issuer": "MeetLines",
    "Audience": "MeetLines",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7
  },

  "Email": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587,
    "SmtpUser": "${EMAIL_SMTP_USER}",
    "SmtpPassword": "${EMAIL_SMTP_PASSWORD}",
    "FromEmail": "${EMAIL_FROM}",
    "FromName": "MeetLines"
  },

  "Frontend": {
    "Url": "http://localhost:3000"
  },

  "GeoIp": {
    "DatabasePath": "GeoIp/GeoLite2-City.mmdb",
    "CacheTtlSeconds": 86400
  },

  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    },
    "Facebook": {
      "AppId": "${FB_APP_ID}",
      "AppSecret": "${FB_APP_SECRET}"
    }
  },

  "Multitenancy": {
    "BaseDomain": "${MULTITENANCY_BASE_DOMAIN}",
    "Protocol": "${MULTITENANCY_PROTOCOL}",
    "Enabled": true
  },

  "Webhooks": {
    "BaseUrl": "${WEBHOOK_BASE_URL}",
    "N8nBaseUrl": "${N8N_BASE_URL}"
  }
}
